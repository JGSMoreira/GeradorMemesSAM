<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="599.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.com.sam.control.ImagemController">
   <children>
      <Pane fx:id="painelmeme" prefHeight="600.0" prefWidth="600.0">
         <children>
            <ImageView fx:id="imgfoto" fitHeight="600.0" fitWidth="600.0" pickOnBounds="true">
               <image>
                  <Image url="@../../../../img/irineu.jpg" />
               </image>
            </ImageView>
            <Label fx:id="lblsuperior" alignment="TOP_CENTER" contentDisplay="CENTER" layoutX="23.0" prefHeight="254.0" prefWidth="555.0" text="texto superior" textAlignment="CENTER" textFill="WHITE" wrapText="true">
               <font>
                  <Font name="Impact" size="62.0" />
               </font>
            </Label>
            <Label fx:id="lblinferior" alignment="BOTTOM_CENTER" contentDisplay="CENTER" layoutX="23.0" layoutY="346.0" prefHeight="254.0" prefWidth="555.0" text="texto inferior" textAlignment="CENTER" textFill="WHITE" wrapText="true">
               <font>
                  <Font name="Impact" size="62.0" />
               </font>
            </Label>
            <ImageView fitHeight="179.0" fitWidth="212.0" layoutX="23.0" layoutY="368.0" opacity="0.68" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../img/logo.png" />
               </image>
            </ImageView>
         </children>
      </Pane>
      <Pane layoutY="600.0" prefHeight="44.0" prefWidth="600.0" style="-fx-background-color: black;">
         <children>
            <Button layoutX="488.0" layoutY="10.0" mnemonicParsing="false" onAction="#exportarMeme" text="Exportar Meme" />
            <Button layoutX="14.0" layoutY="10.0" mnemonicParsing="false" onAction="#alterarImagem" text="Alterar Imagem" />
         </children>
      </Pane>
   </children>
</AnchorPane>
